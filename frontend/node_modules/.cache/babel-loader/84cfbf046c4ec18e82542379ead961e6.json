.prenom);\n      let testJob = regName.exec(this.job);\n      let testEmail = regEmail.exec(this.email);\n\n      if (testNom && testPrenom && testJob && testEmail && this.password != \"\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    createAccount: function () {\n      this.$store.dispatch(\"createAccount\", {\n        lastName: this.nom,\n        firstName: this.prenom,\n        job: this.job,\n        email: this.email,\n        password: this.password\n      });\n    }\n  },\n  components: {\n    HeaderLog\n  }\n};","map":{"version":3,"mappings":"AA6CA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,eAAe;AACbC,MAAI,EAAE,YADO;AAEbC,OAAK,EAAE,CAAC,EAAD,CAFM;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,YADF;AAELC,SAAG,EAAE,EAFA;AAGLC,YAAM,EAAE,EAHH;AAILC,SAAG,EAAE,EAJA;AAKLC,WAAK,EAAE,EALF;AAMLC,cAAQ,EAAE,EANL;AAOLC,oBAAc,EAAE;AAPX,KAAP;AASD,GAbY;;AAcbC,SAAO,EAAE;AACPC,iBAAa,EAAE,YAAY;AACzB,UAAIC,OAAM,GAAI,IAAIC,MAAJ,CACZ,6EADY,CAAd;AAGA,UAAIC,OAAM,GAAIF,OAAO,CAACG,IAAR,CAAa,KAAKX,GAAlB,CAAd;AACA,aAAOU,OAAP;AACD,KAPM;AASPE,oBAAgB,EAAE,YAAY;AAC5B,UAAIJ,OAAM,GAAI,IAAIC,MAAJ,CACZ,6EADY,CAAd;AAGA,UAAII,UAAS,GAAIL,OAAO,CAACG,IAAR,CAAa,KAAKV,MAAlB,CAAjB;AACA,aAAOY,UAAP;AACD,KAfM;AAiBPC,iBAAa,EAAE,YAAY;AACzB,UAAIN,OAAM,GAAI,IAAIC,MAAJ,CACZ,6EADY,CAAd;AAGA,UAAIM,OAAM,GAAIP,OAAO,CAACG,IAAR,CAAa,KAAKT,GAAlB,CAAd;AACA,aAAOa,OAAP;AACD,KAvBM;AA0BPC,mBAAe,EAAE,YAAY;AAC3B,UAAIC,QAAO,GAAI,IAAIR,MAAJ,CAAW,gCAAX,CAAf;AACA,UAAIS,SAAQ,GAAID,QAAQ,CAACN,IAAT,CAAc,KAAKR,KAAnB,CAAhB;AACA,aAAOe,SAAP;AACD,KA9BM;AAgCPC,gBAAY,EAAE,YAAY;AACxBC,aAAO,CAACC,GAAR,CAAY,QAAZ;AACA,UAAIJ,QAAO,GAAI,IAAIR,MAAJ,CAAW,gCAAX,CAAf;AACA,UAAIS,SAAQ,GAAID,QAAQ,CAACN,IAAT,CAAc,KAAKR,KAAnB,CAAhB;;AACA,UAAGe,SAAH,EAAc;AACZ,aAAKb,cAAL,IAAuB,IAAvB;AACF,OAFA,MAEO;AACL,aAAKA,cAAL,IAAuB,KAAvB;AACF;AACD,KAzCM;AA2CPiB,iBAAa,EAAE,YAAY;AACzB,UAAI,KAAKlB,QAAL,IAAiB,EAArB,EAAyB;AACvB,eAAO,IAAP;AACF,OAFA,MAEO;AACL,eAAO,KAAP;AACF;AACD,KAjDM;AAmDPmB,mBAAe,EAAE,YAAY;AAC3B,UAAIf,OAAM,GAAI,IAAIC,MAAJ,CACZ,6EADY,
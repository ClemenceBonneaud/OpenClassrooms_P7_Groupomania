{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"ProfilOther\",\n  path: \"/profil/:id\",\n  props: [\"\"],\n\n  data() {\n    return {\n      titre: \"Profil de \",\n      sousTitre: \"Publications\",\n      profil: {\n        nom: \"\",\n        prenom: \"\",\n        job: \"\"\n      },\n      article: {\n        titre: \"\",\n        contenu: \"\"\n      }\n    };\n  },\n\n  mounted() {\n    let params = new URL(window.location).searchParams;\n    let id = params.get(\"id\");\n    console.log(id);\n    instance.get(`auth/infos/${id}`).then(response => {\n      console.log(response);\n      this.profil.nom = response.data.userFound.lastName;\n      this.profil.prenom = response.data.userFound.firstName;\n      this.profil.job = response.data.userFound.job;\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    partagerPublication() {\n      console.log(\"publié\"); //////////////////////////////////////////////////////////\n    },\n\n    publicationCheck: function () {\n      //////////////////////////////////////////////////////////\n      return false;\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,MAAI,EAAE,aAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,YADF;AAELC,eAAS,EAAE,cAFN;AAGLC,YAAM,EAAE;AACNC,WAAG,EAAE,EADC;AAENC,cAAM,EAAE,EAFF;AAGNC,WAAG,EAAE;AAHC,OAHH;AAQLC,aAAO,EAAE;AACPN,aAAK,EAAE,EADA;AAEPO,eAAO,EAAE;AAFF;AARJ,KAAP;AAaD,GAlBY;;AAmBbC,SAAO,GAAG;AACR,QAAIC,MAAK,GAAI,IAAIC,GAAJ,CAAQC,MAAM,CAACC,QAAf,EAAyBC,YAAtC;AACA,QAAIC,EAAC,GAAIL,MAAM,CAACM,GAAP,CAAW,IAAX,CAAT;AAEAC,WAA
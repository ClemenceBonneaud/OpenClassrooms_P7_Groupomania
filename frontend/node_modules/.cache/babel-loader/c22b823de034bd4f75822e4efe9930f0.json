{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\";\nimport { mapGetters, mapActions } from \"vuex\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n\n  data() {\n    return {\n      profil: {\n        prenom: \"clemence\",\n        id: \"1\"\n      }\n    };\n  },\n\n  methods: { ...mapActions([\"incrementMonth\"]),\n\n    incrementMonthFirst() {\n      this.$store.commit(\"INCREMENT_MONTH\");\n    },\n\n    goToProfil: function () {\n      const user = this.$store.state.userId;\n      instance.get(`auth/infos/${user}`).then(response => {\n        console.log(response);\n        this.profil.nom = response.data.userFound.lastName;\n        this.profil.prenom = response.data.userFound.firstName;\n        this.profil.job = response.data.userFound.job;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  components: {\n    HeaderHome\n  },\n  computed: { ...mapGetters([\"formattedDate\", \"formattedTime\"])\n  }\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,SAASC,UAAT,EAAqBC,UAArB,QAAuC,MAAvC,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,GAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AACNC,cAAM,EAAE,UADF;AAENC,UAAE,EAAC;AAFG;AADH,KAAP;AAMD,GAXY;;AAYbC,SAAO,EAAE,EACP,GAAGb,UAAU,CAAC,CAAC,gBAAD,CAAD,CADN;;AAEPc,uBAAmB,GAAG;AACpB,WAAKC,MAAL,CAAYC,MAAZ,CAAmB,iBAAnB;AACD,KAJM;;AAKPC,cAAU,EAAE,YAAW;AACrB,YAAMC,IAAG,GAAI,KAAKH,MAAL,CAAYI,KAAZ,CAAkBC,MAA/B;AACFjB,cAAO,CACJkB,GADH,CACQ,cAAaH,IAAK,EAD1B,EAEGI,IAFH,CAESC,QAAD,IAAc;AAClBC,eAAO,CAACC,GAAR,CAAYF,QAAZ;AACA,aAAKb,MAAL,CAAYgB,GAAZ,GAAkBH,QAAQ,CAACd,IAAT,CAAckB,SAAd,CAAwBC,QAA1C;AACA,aAAKlB,MAAL,CAAYC,MAAZ,GAAqBY,QAAQ,CAACd,IAAT
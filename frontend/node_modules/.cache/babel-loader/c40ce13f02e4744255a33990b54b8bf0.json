lor: $colour-tertiary;\n    display: flex;\n    justify-content: space-between;\n    & figure {\n        height: 50px;\n        padding-top: 30px;\n    }\n    & img {\n        height: 100%;\n    }\n    & a {\n        text-align: center;\n        height: 100%;\n        width: 100%;\n        border-left: 2px solid $colour-secondary;\n        transition: all 500ms;\n        &:hover {\n            background-color: $colour-primary-light;\n            font-weight: bolder;\n            font-size: larger;\n        }\n    }\n    & nav {\n        height: 100%;\n        width: 40%;\n        display: flex;\n        flex-direction: row;\n        justify-content: space-around;\n        align-items: center;\n        line-height: 150px;\n    }\n    @media screen and (max-width: 768px) {\n        flex-direction: column;\n        align-items: center;\n        height: auto;\n        & figure {\n            width: fit-content ;\n            height: auto;\n            padding-top:    20px;\n            padding-bottom: 20px;\n        }\n        & img {\n            width: 100%;\n            height: auto;\n        }\n        & nav {\n            line-height: 60px;\n            width: 100%;\n            border-top: 2px solid $colour-secondary;\n            border-right: 2px solid $colour-secondary;\n        }\n    }\n}\n\n// Bloc\n\n.bloc {\n    background-color: $colour-tertiary;\n    padding: 30px;\n    width: 50%;\n    margin-left: auto;\n    margin-right: auto;\n    border-radius: 20px;\n}\n\n// Bouton\n\n.bouton {\n    padding: 10px;\n    margin-top: 20px;\n    border-radius: 10px;\n    background-color: $colour-primary-light;\n    display: flex;\n    width: 95px;\n    margin-left: 70%;\n    &:hover {\n        background-color: $colour-primary;\n    }\n    @media screen and (max-width: 768px) {\n        margin-left: unset;\n        align-self: center;\n    }\n}\n\n// Form\n\nform {\n    display: flex;\n    flex-direction: column;\n    width: 70%;\n    margin-left: auto;\n    margin-right: auto;\n    & label {\n        padding: 10px;\n    }\n    & input {\n        margin-bottom: 20px;\n        padding-top: 5px;\n        padding-bottom: 5px;\n    }\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module"}
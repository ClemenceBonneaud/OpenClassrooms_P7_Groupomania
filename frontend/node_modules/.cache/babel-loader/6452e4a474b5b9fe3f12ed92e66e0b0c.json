nce.delete(`auth/delete/${user}`).then(response => {\n          console.log(response);\n          this.$router.push(\"/signup\"); // Redirection vers page d'inscription\n        }).catch(error => {\n          console.log(error);\n        });\n      }\n    },\n    publicationCheck: function () {\n      if (this.publication == true) {\n        return true;\n      }\n\n      return false;\n    },\n    supprimerArticle: function () {\n      console.log(\"///////\");\n      /*       let confirmation = confirm(\n              \"Êtes-vous certain de vouloir supprimer votre article ?\"\n            );\n            console.log(\"-------------\");\n            if (confirmation) {\n              console.log(\"titre : \"); */\n\n      /*         const article = ;\n              instance\n                .delete(`auth/delete/${user}`)\n                .then((response) => {\n                  console.log(response);\n                })\n                .catch((error) => {\n                  console.log(error);\n                }); \n            }*/\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AAmDA;AACA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,cADO;AAEbC,MAAI,EAAE,YAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,QADF;AAELC,eAAS,EAAE,cAFN;AAGLC,YAAM,EAAE;AACNC,WAAG,EAAE,EADC;AAENC,cAAM,EAAE,EAFF;AAGNC,WAAG,EAAE;AAHC,OAHH;AAQLC,cAAQ,EAAE,EARL;AASLC,iBAAW,EAAE;AATR,KAAP;AAWD,GAhBY;;AAiBbC,SAAO,GAAG;AACR,UAAMC,MAAK,GAAI,IAAIC,SAAJ,CAAc,qBAAd,CAAf,CADQ,CAGR;;AACAD,UAAM,CAACE,gBAAP,CAAwB,OAAxB,EAAiC,UAAUC,KAAV,EAAiB;AAChDC,aAAO,CAACC,GAAR,CAAY,mBAAZ,EAAiCF,KAAjC;AACD,KAFD,EAJQ,CAQR;;AACA,UAAMG,IAAG,GAAI,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,MAA/B;AACAzB,YAAO,CACJ0B,GADH,CACQ,cAAaJ,IAAK,EAD1B,EAEGK,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKnB,MAAL,CAAYC,GAAZ,GAAkBkB,QAAQ,CAACtB,IAAT,CAAcuB,SAAd,CAAwBC
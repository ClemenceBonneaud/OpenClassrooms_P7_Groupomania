{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n\n  data() {\n    return {\n      articles: []\n    };\n  },\n\n  mounted() {\n    // Remplissage des data avec les infos API\n    instance.get(`article/`).then(response => {\n      let i = 0;\n\n      for (i = 0; i < response.data.length; i += 1) {\n        let article = response.data[i];\n        let rowDate = article.createdAt;\n        let date = rowDate.substr(0, 10);\n        this.articles.push({\n          \"titre\": article.title,\n          \"contenu\": article.text,\n          \"auteur\": article.User.firstName,\n          \"auteurId\": article.UserId,\n          \"date\": date\n        });\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    goToProfil: function (userId) {\n      this.$router.push(`/profil/${userId}`);\n    },\n    partagerArticle: function () {\n      let comment = prompt({\n        title: \"Partager un article\",\n        message: \"Qu'avez-vous à dire sur cet article ?\",\n        okButtonText: \"Partager\",\n        cancelButtonText: \"Annuler\",\n        defaultText: \"Regardez cet article !\"\n      });\n      console.log(comment);\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,GAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE;AADL,KAAP;AAGD,GARY;;AASbC,SAAO,GAAG;AACR;AACAR,YAAO,CACJS,GADH,CACQ,UADR,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,UAAIC,IAAI,CAAR;;AACA,WAAKA,IAAI,CAAT,EAAYA,IAAID,QAAQ,CAACL,IAAT,CAAcO,MAA9B,EAAsCD,KAAK,CAA3C,EAA8C;AAC5C,YAAIE,
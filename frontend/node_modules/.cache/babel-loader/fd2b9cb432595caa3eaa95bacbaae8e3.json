lhost:3000/api/\",\n});\n\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n  data() {\n    return {\n      profil: {\n        prenom: \"\",\n        id: \"\",\n      },\n      articles: [\n        {\n          titre: \"Titre\",\n          contenu: \"Contenu\",\n          auteur: \"Auteur\",\n          date: \"date\"\n        },\n        {\n          titre: \"Titre2\",\n          contenu: \"Contenu2\",\n          auteur: \"Auteur2\",\n          date: \"date\"\n        },\n      ]\n    };\n  },\n  mounted() {\n    // Remplissage des data avec les infos API\n    // const user = this.$store.state.userId;\n    instance\n      .get(`article/`)\n      .then((response) => {\n        console.log(response);\n        console.log(response.data);\n        console.log(response.data.length);\n        for (i = 0; i < response.data.length; i += 1) {\n          console.log(response.data.title);\n        }\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    goToProfil: function () {\n      this.$router.push(`/profil/${this.profil.id}`);\n    },\n    partagerArticle: function () {\n      console.log(\"partage\");\n    }\n  },\n  components: { HeaderHome },\n};\n</script>\n\n<style lang=\"scss\">\na {\n  color: black;\n  text-decoration: none;\n}\n\n#timeStamp {\n  display: flex;\n  justify-content: space-between;\n}\n\n.bouton {\n  margin: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
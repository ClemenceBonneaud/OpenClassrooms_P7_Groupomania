e \",\n      sousTitre: \"Publications\",\n      profil: {\n        nom: \"\",\n        prenom: \"\",\n        job: \"\",\n      },\n      article: {\n        titre: \"\",\n        contenu: \"\",\n      },\n    };\n  },\n  mounted() {\n    const user = this.$store.state.userId;\n    instance\n      .get(`auth/infos/${user}`)\n      .then((response) => {\n        console.log(response);\n        this.profil.nom = response.data.userFound.lastName;\n        this.profil.prenom = response.data.userFound.firstName;\n        this.profil.job = response.data.userFound.job;\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    partagerPublication() {\n      console.log(\"publié\"); //////////////////////////////////////////////////////////\n    },\n    publicationCheck: function () { //////////////////////////////////////////////////////////\n      return false;\n    },\n  },\n  components: { HeaderHome },\n};\n</script>\n\n<style lang=\"scss\">\n.profilEdit {\n  display: flex;\n  justify-content: space-evenly;\n  & a {\n    margin: 0;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n\n  data() {\n    return {\n      profil: {\n        prenom: \"\",\n        id: \"\"\n      },\n      articles: [{\n        titre: \"Titre\",\n        contenu: \"Contenu\",\n        auteur: \"Auteur\",\n        date: \"date\"\n      }, {\n        titre: \"Titre2\",\n        contenu: \"Contenu2\",\n        auteur: \"Auteur2\",\n        date: \"date\"\n      }]\n    };\n  },\n\n  mounted() {\n    // Remplissage des data avec les infos API\n    // const user = this.$store.state.userId;\n    instance.get(`article/`).then(response => {\n      let i = 0;\n\n      for (i = 0; i < response.data.length; i += 1) {\n        console.log(response.data[i].title);\n        let article = response.data[i];\n        let date = article.createdAt; ////////////////////////////////\n\n        this.articles += {\n          titre: article.title,\n          contenu: article.text,\n          auteur: article.User.firstName,\n          date: date\n        };\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    goToProfil: function () {\n      this.$router.push(`/profil/${this.profil.id}`);\n    },\n    partagerArticle: function () {\n      console.log(\"partage\");\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,GAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AACNC,cAAM,EAAE,EADF;AAENC,UAAE,EAAE;AAFE,OADH;AAKLC,cAAQ,EAAE,CACR;AACEC,aAAK,EAAE,OADT;AAEEC,eAAO,EAAE,SAFX;AAGEC,cAAM,EAAE,QAHV;AAIEC,YAAI,EAAE;
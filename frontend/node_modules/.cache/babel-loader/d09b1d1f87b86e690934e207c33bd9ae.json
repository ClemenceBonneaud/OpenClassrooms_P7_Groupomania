n    <h4>{{ article.titre }}</h4>\n    <p>{{ article.contenu }}</p>\n\n    <div id=\"timeStamp\">\n      <p>Publi√© le {{ article.date }}</p>\n      <a class=\"bouton\" @click=\"partagerArticle()\">Partager</a>\n    </div>\n  </div>\n</template>\n\n<script>\nimport HeaderHome from \"@/components/HeaderHome.vue\";\n\n// Axios\nconst axios = require(\"axios\");\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\",\n});\n\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n  data() {\n    return {\n      profil: {\n        prenom: \"\",\n        id: \"\",\n      },\n      articles: [\n        {\n          titre: \"Titre\",\n          contenu: \"Contenu\",\n          auteur: \"Auteur\",\n          date: \"date\"\n        },\n        {\n          titre: \"Titre2\",\n          contenu: \"Contenu2\",\n          auteur: \"Auteur2\",\n          date: \"date\"\n        },\n      ]\n    };\n  },\n  mounted() {\n    // Remplissage des data avec les infos API\n    // const user = this.$store.state.userId;\n    instance\n      .get(`article/`)\n      .then((response) => {\n        let i = 0;\n        for (i = 0; i < response.data.length; i += 1) {\n          console.log(response.data[i].title);\n          let article = response.data[i];\n          let date = article.createdAt; ////////////////////////////////\n          this.articles += {\n            titre: article.title,\n            contenu: article.text,\n            auteur: article.User.firstName,\n            date: date\n          }\n        }\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    goToProfil: function () {\n      this.$router.push(`/profil/${this.profil.id}`);\n    },\n    partagerArticle: function () {\n      console.log(\"partage\");\n    }\n  },\n  components: { HeaderHome },\n};\n</script>\n\n<style lang=\"scss\">\na {\n  color: black;\n  text-decoration: none;\n}\n\n#timeStamp {\n  display: flex;\n  justify-content: space-between;\n}\n\n.bouton {\n  margin: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
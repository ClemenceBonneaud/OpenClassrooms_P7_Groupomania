{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n\n  data() {\n    return {\n      articles: []\n    };\n  },\n\n  mounted() {\n    // Remplissage des data avec les infos API\n    instance.get(`article/`).then(response => {\n      let i = 0;\n\n      for (i = 0; i < response.data.length; i += 1) {\n        let article = response.data[i];\n        let rowDate = article.createdAt;\n        let date = rowDate.substr(0, 10);\n        this.articles.push({\n          \"titre\": article.title,\n          \"contenu\": article.text,\n          \"auteur\": article.User.firstName,\n          \"auteurId\": article.UserId,\n          \"date\": date,\n          \"partages\": article.shares\n        });\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    goToProfil: function (userId) {\n      this.$router.push(`/profil/${userId}`);\n    },\n    partagerArticle: function () {\n      prompt(\"Qu'avez-vous à dire sur cet article ?\", \"Regardez cet article !\").then(result => {\n        console.log(`Dialog result: ${result.result}, text: ${result.text}`);\n      });\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,GAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE;AADL,KAAP;AAGD,GARY;;AASbC,SAAO,GAAG;AACR;AACAR,YAAO,CACJS,GADH,CACQ,UADR,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,UAAIC,IAAI,CAAR;;AACA,WAAKA,IAAI,CAAT,EAAYA,IAAID,QAAQ,CAACL,IAAT,CAAcO,MAA9B,EAAsCD,KAAK,CAA3C,EAA8C;AAC5C,YAAIE,OAAM,GAAIH,QAAQ,CAACL,IAAT,CAAcM,CAAd,CAAd;AACA,YAAIG,OAAM,GAAID,OAAO,CAACE,SAAtB;AACA,YAAIC,IAAG,GAAIF,OAAO,CAACG,MAAR,CAAe,CAAf,EAAkB,EAAlB,CAAX;AACA,aAAKX,QAAL,CAAcY,IAAd,CACE;AACE,mBAASL,OAAO,CAACM,KADnB;AAEE,qBAAWN,OAAO,CAACO,IAFrB;AAGE,oBAAUP,OAAO,CAACQ,IAAR,CAAaC,SAHzB;AAIE,sBAAYT,OAAO,CAACU,MAJtB;AAKE,kBAAQP,IALV;AAME,sBAAYH,OAAO,CAACW;AANtB,SADF;AASF;AACD,KAlBH,EAmBGC,KAnBH,CAmBUC,KAAD,IAAW;AAChBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,KArBH;AAsBD,GAjCY;;AAkCbG,SAAO,EAAE;AACPC,cAAU,EAAE,UAAUC,MAAV,EAAkB;AAC5B,WAAKC,OAAL,CAAad,IAAb,CAAmB,WAAUa,MAAO,EAApC;AACD,KAHM;AAIPE,mBAAe,EAAE,YAAY;AAC3BC,YAAM,CAAC,uCAAD,EAA0C,wBAA1C,CAAN,CACEzB,IADF,CACO0B,MAAK,IAAK;AACfR,eAAO,CAACC,GAAR,CAAa,kBAAiBO,MAAM,CAACA,MAAO,WAAUA,MAAM,CAACf,IAAK,EAAlE;AACD,OAHD;AAIF;AATO,GAlCI;AA6CbgB,YAAU,EAAE;AAAExC;AAAF;AA7CC,CAAf","names":["HeaderHome","axios","require","instance","create","baseURL","name","path","props","data","articles","mounted","get","then","response","i","length","article","rowDate","createdAt","date","substr","push","title","text","User","firstName","UserId","shares","catch","error","console","log","methods","goToProfil","userId","$router","partagerArticle","prompt","result","components"],"sourceRoot":"","sources":["/Users/clemencebonneaud/Desktop/Dev/OpenClassrooms/Cours/OC_P7/code/frontend/groupomania-v1.0/src/views/HomeView.vue"],"sourcesContent":["<!-- ////////////////////////////////////////////////// -->\n<!--                      TEMPLATE                      -->\n<!-- ////////////////////////////////////////////////// -->\n\n<template>\n  <HeaderHome />\n\n  <h1>Forum Articles</h1>\n\n  <div class=\"bloc\" v-for=\"article in articles\" v-bind:key=\"article.titre\">\n    <h3>\n      <a id=\"lienProfil\" title=\"Voir le profil\" @click=\"goToProfil(article.auteurId)\">{{\n        article.auteur\n      }}</a>\n    </h3>\n\n    <h4>{{ article.titre }}</h4>\n    <p>{{ article.contenu }}</p>\n\n    <div id=\"timeStamp\">\n      <p>Publié le {{ article.date }}</p>\n      <div>\n      <a class=\"bouton\" @click=\"partagerArticle()\">Partager</a>\n      <p id=\"shares\">{{ article.partages }} Partages</p>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<!-- ////////////////////////////////////////////////// -->\n<!--                       SCRIPT                       -->\n<!-- ////////////////////////////////////////////////// -->\n\n<script>\nimport HeaderHome from \"@/components/HeaderHome.vue\";\n\n// Axios\nconst axios = require(\"axios\");\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\",\n});\n\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n  data() {\n    return {\n      articles: []\n    };\n  },\n  mounted() {\n    // Remplissage des data avec les infos API\n    instance\n      .get(`article/`)\n      .then((response) => {\n        let i = 0;\n        for (i = 0; i < response.data.length; i += 1) {\n          let article = response.data[i];\n          let rowDate = article.createdAt;\n          let date = rowDate.substr(0, 10);\n          this.articles.push(\n            {\n              \"titre\": article.title,\n              \"contenu\": article.text,\n              \"auteur\": article.User.firstName,\n              \"auteurId\": article.UserId,\n              \"date\": date,\n              \"partages\": article.shares,\n            });\n        }\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    goToProfil: function (userId) {\n      this.$router.push(`/profil/${userId}`);\n    },\n    partagerArticle: function () {\n      prompt(\"Qu'avez-vous à dire sur cet article ?\", \"Regardez cet article !\",\n      ).then(result => {\n        console.log(`Dialog result: ${result.result}, text: ${result.text}`)\n      });\n    }\n  },\n  components: { HeaderHome },\n};\n</script>\n\n<!-- ////////////////////////////////////////////////// -->\n<!--                       STYLE                        -->\n<!-- ////////////////////////////////////////////////// -->\n\n<style lang=\"scss\">\na {\n  color: black;\n  text-decoration: none;\n}\n\n#timeStamp {\n  display: flex;\n  justify-content: space-between;\n}\n\n.bouton {\n  margin: 0;\n}\n\n#shares {\n  font-size: 12px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}
{"ast":null,"code":"import HeaderHome from \"@/components/HeaderHome.vue\"; // Axios\n\nconst axios = require(\"axios\");\n\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\"\n});\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n\n  data() {\n    return {\n      profil: {\n        prenom: \"\",\n        id: \"\"\n      },\n      articles: []\n    };\n  },\n\n  mounted() {\n    // Remplissage des data avec les infos API\n    instance.get(`article/`).then(response => {\n      let i = 0;\n\n      for (i = 0; i < response.data.length; i += 1) {\n        let article = response.data[i];\n        let rowDate = article.createdAt;\n        let date = rowDate.substr(0, 10);\n        this.articles.push({\n          \"titre\": article.title,\n          \"contenu\": article.text,\n          \"auteur\": article.User.firstName,\n          \"date\": date\n        });\n      }\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    goToProfil: function () {\n      this.$router.push(`/profil/${this.profil.id}`);\n    },\n    partagerArticle: function () {\n      console.log(\"partage\");\n    }\n  },\n  components: {\n    HeaderHome\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,UAAP,MAAuB,6BAAvB,C,CAEA;;AACA,MAAMC,KAAI,GAAIC,OAAO,CAAC,OAAD,CAArB;;AACA,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAN,CAAa;AAC5BC,SAAO,EAAE;AADmB,CAAb,CAAjB;AAIA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,MAAI,EAAE,GAFO;AAGbC,OAAK,EAAE,CAAC,EAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AACNC,cAAM,EAAE,EADF;AAENC,UAAE,EAAE;AAFE,OADH;AAKLC,cAAQ,EAAE;AALL,KAAP;AAOD,GAZY;;AAabC,SAAO,GAAG;AACR;AACAX,YAAO,CACJY,GADH,CACQ,UADR,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,UAAIC,IAAI,CAAR;;AACA,WAAKA,IAAI,CAAT,EAAYA,IAAID,QAAQ,CAACR,IAAT,CAAcU,MAA9B,EAAsCD,KAAK,CAA3C,EAA8C;AAC5C,YAAIE,OAAM,GAAIH,QAAQ,CAACR,IAAT,CAAcS,CAAd,CAAd;AACA,YAAIG,OAAM,GAAID,OAAO,CAACE,SAAtB;AACA,YAAIC,IAAG,GAAIF,OAAO,CAACG,MAAR,CAAe,CAAf,EAAkB,EAAlB,CAAX;AACA,aAAKX,QAAL,CAAcY,IAAd,CACE;AACA,mBAASL,OAAO,CAACM,KADjB;AAEA,qBAAWN,OAAO,CAACO,IAFnB;AAGA,oBAAUP,OAAO,CAACQ,IAAR,CAAaC,SAHvB;AAIA,kBAAQN;AAJR,SADF;AAMF;AACD,KAfH,EAgBGO,KAhBH,CAgBUC,KAAD,IAAW;AAChBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,KAlBH;AAmBD,GAlCY;;AAmCbG,SAAO,EAAE;AACPC,cAAU,EAAE,YAAY;AACtB,WAAKC,OAAL,CAAaX,IAAb,CAAmB,WAAU,KAAKf,MAAL,CAAYE,EAAG,EAA5C;AACD,KAHM;AAIPyB,mBAAe,EAAE,YAAY;AAC3BL,aAAO,CAACC,GAAR,CAAY,SAAZ;AACF;AANO,GAnCI;AA2CbK,YAAU,EAAE;AAAEtC;AAAF;AA3CC,CAAf","names":["HeaderHome","axios","require","instance","create","baseURL","name","path","props","data","profil","prenom","id","articles","mounted","get","then","response","i","length","article","rowDate","createdAt","date","substr","push","title","text","User","firstName","catch","error","console","log","methods","goToProfil","$router","partagerArticle","components"],"sourceRoot":"","sources":["/Users/clemencebonneaud/Desktop/Dev/OpenClassrooms/Cours/OC_P7/code/frontend/groupomania-v1.0/src/views/HomeView.vue"],"sourcesContent":["<!-- ////////////////////////////////////////////////// -->\n<!--                      TEMPLATE                      -->\n<!-- ////////////////////////////////////////////////// -->\n\n<template>\n  <HeaderHome />\n\n  <h1>Forum Articles</h1>\n\n  <div class=\"bloc\" v-for=\"article in articles\" v-bind:key=\"article.titre\">\n    <h3>\n      <a id=\"lienProfil\" title=\"Voir le profil\" @click=\"goToProfil()\">{{\n        article.auteur\n      }}</a>\n    </h3>\n\n    <h4>{{ article.titre }}</h4>\n    <p>{{ article.contenu }}</p>\n\n    <div id=\"timeStamp\">\n      <p>Publi√© le {{ article.date }}</p>\n      <a class=\"bouton\" @click=\"partagerArticle()\">Partager</a>\n    </div>\n  </div>\n</template>\n\n<!-- ////////////////////////////////////////////////// -->\n<!--                       SCRIPT                       -->\n<!-- ////////////////////////////////////////////////// -->\n\n<script>\nimport HeaderHome from \"@/components/HeaderHome.vue\";\n\n// Axios\nconst axios = require(\"axios\");\nconst instance = axios.create({\n  baseURL: \"http://localhost:3000/api/\",\n});\n\nexport default {\n  name: \"HomeView\",\n  path: \"/\",\n  props: [\"\"],\n  data() {\n    return {\n      profil: {\n        prenom: \"\",\n        id: \"\",\n      },\n      articles: []\n    };\n  },\n  mounted() {\n    // Remplissage des data avec les infos API\n    instance\n      .get(`article/`)\n      .then((response) => {\n        let i = 0;\n        for (i = 0; i < response.data.length; i += 1) {\n          let article = response.data[i];\n          let rowDate = article.createdAt;\n          let date = rowDate.substr(0, 10)\n          this.articles.push(\n            { \n            \"titre\": article.title, \n            \"contenu\": article.text, \n            \"auteur\": article.User.firstName, \n            \"date\": date })\n        }\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  },\n  methods: {\n    goToProfil: function () {\n      this.$router.push(`/profil/${this.profil.id}`);\n    },\n    partagerArticle: function () {\n      console.log(\"partage\");\n    }\n  },\n  components: { HeaderHome },\n};\n</script>\n\n<!-- ////////////////////////////////////////////////// -->\n<!--                       STYLE                        -->\n<!-- ////////////////////////////////////////////////// -->\n\n<style lang=\"scss\">\na {\n  color: black;\n  text-decoration: none;\n}\n\n#timeStamp {\n  display: flex;\n  justify-content: space-between;\n}\n\n.bouton {\n  margin: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}